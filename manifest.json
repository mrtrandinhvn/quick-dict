{
    "name": "Unofficial Oxford Learner's Dictionaries",
    "version": "1.0.0",
    "description": "This product uses the data of Oxford Learner's Dictionaries but it is a fan-made extension. I'm just a regular user and I find that having an extension for that site is extreamly useful.",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.facebook.com/*",
                "http://www.facebook.com/*"
            ],
            "css": [ "css/EmoStyle.css" ],
            "js": [
                "./built/js/"
            ]
        }
    ],
    "browser_action": {
        "default_icon": {
            "19": "icons/icon16.png",
            "38": "icons/icon48.png"
        },
        "default_popup": "default_popup.html"
    },
    "manifest_version": 2,
    "author": "mrtrandinhvn@gmail.com"
}