{
    "name": "Quick Dict - Từ điển nhanh",
    "version": "1.1.6",
    "description": "Allow you to quickly open a translate site after you select some text by mouse",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon64.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [{
        "matches": [
            "*://*/*"
        ],
        "css": [
            "./built/content_scripts/css/index.css"
        ],
        "js": [
            "./built/content_scripts/scripts/index.js"
        ]
    }],
    "action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon64.png"
        },
        "default_popup": "src/browser_action/index.html"
    },
    "web_accessible_resources": [{
        "resources": ["resources/images/*"],
        "matches": ["*://*/*"]
    }],
    "manifest_version": 3,
    "author": "mrtrandinhvn@gmail.com"
}