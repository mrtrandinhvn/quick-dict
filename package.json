{
  "name": "unofficial-oxford-learner-s-dictionaries",
  "version": "1.0.0",
  "description": "This product uses the data of Oxford Learner's Dictionaries but it is a fan-made extension. I'm just a regular user and I find that having an extension for that site is extreamly useful.",
  "repository": {
    "git": ""
  },
  "scripts": {
    "test": "jest",
    "dev": "webpack --watch --progress",
    "dev-less": "webpack --watch --progress --config webpack-styles.config.js",
    "build": "webpack -p --progress --env.production",
    "build-less": "webpack --p --progress --config webpack-styles.config.js"
  },
  "author": "mrtrandinhvn@gmail.com",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "devDependencies": {
    "@types/jest": "^21.1.4",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "latest",
    "eslint": "^4.1.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "jest": "^21.2.1",
    "node-sass": "latest",
    "sass-loader": "latest",
    "style-loader": "latest",
    "ts-jest": "^21.1.3",
    "ts-loader": "^3.0.3",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3",
    "uglify-js": "^3.0.26",
    "url-loader": "^0.6.2",
    "webpack": "^3.4.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}