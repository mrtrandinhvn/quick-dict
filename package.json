{
  "name": "quick-dict",
  "version": "1.0.0",
  "description": "A chrome extension that allows user to quickly search for a word in his favor dictionary website",
  "repository": {
    "git": ""
  },
  "scripts": {
    "test": "jest --watch",
    "dev": "webpack --watch --progress",
    "dev-less": "webpack --watch --progress --config webpack-styles.config.js",
    "build": "webpack -p --progress --env.production",
    "build-less": "webpack --p --progress --config webpack-styles.config.js"
  },
  "author": "mrtrandinhvn@gmail.com",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  },
  "devDependencies": {
    "@types/jest": "^21.1.6",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^21.2.1",
    "node-sass": "^4.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.2.2",
    "ts-loader": "^3.1.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "uglify-js": "^3.1.9",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
